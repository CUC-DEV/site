mixin sidemenu(menus)
	ul.collapsible(data-collapsible='accordion').popout
		each menu in menus
			li
				.collapsible-header
					i.material-icons=menu.icon
					=menu.name
				.collapsible-body(id=menu.id)
					.collection
						//只实现了动态载入，具体内容还需继续修改
						if !categories
							a.collection-item(href='#!') 空
						each category in categories
							a.collection-item(href='#!')=category.name
						