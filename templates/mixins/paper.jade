mixin paper(papers)
    if papers && papers.length
        .collection
            each paper in papers
                .collection-item #{paper.title}
                    if paper.file.filename
                        a.secondary-content(href = paperPrefix + paper.file.filename download).material-icons play_for_work
            if papers.length<10
                - for (var i = 0; i < 10-papers.length; ++i) { 
                    //img.circle(src='images/paper/paper.jpg', alt='')   
                    .collection-item 暂无资料
                - }
                    